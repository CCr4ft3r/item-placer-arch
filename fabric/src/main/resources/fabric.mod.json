{
  "schemaVersion": 1,
  "id": "item-placer",
  "version": "${version}",
  "name": "${name}",
  "description": "${description}",
  "authors": [
    "akicater"
  ],
  "contact": {
    "discord": "https://fabricmc.net/",
    "github": "https://github.com/kiber-pacan/item-placer"
  },
  "license": "MIT",
  "icon": "assets/item-placer/logo.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.akicater.fabric.ItemPlacerFabric"
    ],
    "client": [
      "com.akicater.fabric.client.ItemPlacerFabricClient"
    ],
    "modmenu": [
      "com.akicater.fabric.client.ModMenuApiImpl"
    ]
  },
  "mixins": [
    "itemplacer.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader}",
    "minecraft": "~${minecraft_version}",
    "java": ">=${java}",
    "architectury": ">=${architectury_api_version}",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}
